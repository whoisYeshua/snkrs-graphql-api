{
  "name": "snkrs-graphql-api",
  "version": "0.0.1",
  "description": "Simple SNKRS GraphQL API for upcomming releases",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tsx src/index.ts",
    "dev:watch": "tsx watch src/index.ts",
    "start": "npm run dev:watch",
    "test": "echo \"Error: no test specified\" && exit 1",
    "codegen": "graphql-codegen",
    "codegen:check": "npm run codegen -- --check",
    "codegen:watch": "npm run codegen -- --watch",
    "format": "npm run format:eslint && npm run format:prettier",
    "format:eslint": "npm run lint:eslint -- --fix",
    "format:prettier": "npm run lint:prettier -- --write",
    "lint": "npm run lint:eslint && npm run lint:prettier && npm run lint:typescript",
    "lint:eslint": "eslint \"**/*.{js,ts,[cm][jt]s}\" --debug",
    "lint:prettier": "prettier \"**/*.{json,md,js,ts,[cm][jt]s}\" --check",
    "lint:typescript": "tsc --noEmit",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/whoisYeshua/snkrs-graphql-api.git"
  },
  "keywords": [
    "graphql",
    "snkrs",
    "nike"
  ],
  "author": "whoisYeshua",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/whoisYeshua/snkrs-graphql-api/issues"
  },
  "homepage": "https://github.com/whoisYeshua/snkrs-graphql-api#readme",
  "engines": {
    "node": ">=18.13",
    "npm": ">= 8.1"
  },
  "dependencies": {
    "@graphql-tools/load-files": "^6.6.1",
    "@graphql-tools/schema": "^9.0.13",
    "graphql": "^16.6.0",
    "graphql-yoga": "^3.3.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.2",
    "@commitlint/config-conventional": "^17.4.2",
    "@graphql-codegen/cli": "^2.16.4",
    "@prisma/client": "^4.9.0",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "@typescript-eslint/parser": "^5.48.2",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-unicorn": "^45.0.2",
    "husky": "^8.0.0",
    "lint-staged": "^13.1.0",
    "prettier": "^2.8.3",
    "prisma": "^4.9.0",
    "tsx": "^3.12.2",
    "typescript": "^4.9.4"
  }
}
